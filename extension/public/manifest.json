{
  "manifest_version": 3,
  "name": "Smart Bookmark",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "storage"
  ],

  "host_permissions": [
    "https://*.supabase.co/*",
    "wss://*.supabase.co/*"
  ],

  "background": {
    "service_worker": "background.js"
  }
}
